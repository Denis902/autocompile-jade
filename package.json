{
  "name": "autocompile-jade",
  "main": "./lib/autocompile-jade",
  "version": "0.0.2",
  "description": "Auto compiles jade in atom",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulpflug/autocompile-jade"
  },
  "activationCommands": {
    "atom-text-editor[data-grammar~=\"source\"][data-grammar~=\"jade\"]": [
      "core:save"
    ]
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-simple-logger": "~0.0.1",
    "jade": "^1.11.0"
  },
  "dev-dependencies": {
    "atom-package-reloader": "~0.0.2"
  }
}
